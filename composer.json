{
  "name": "webdevstudios/github-action-testing",
  "description": "A sandbox for testing Github Actions.",
  "type": "project",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "WebDevStudios",
      "email": "contact@webdevstudios.com"
    }
  ],
  "config": {
    "platform": {
      "php": "7.4"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    {
      "type": "composer",
      "url": "https://packages.wdslab.com"
    }
  ],
  "extra": {
    "installer-paths": {
      "plugins/{$name}/": [
        "type:wordpress-plugin"
      ],
      "mu-plugins/{$name}/": [
        "type:wordpress-muplugin"
      ],
      "themes/{$name}/": [
        "type:wordpress-theme"
      ]
    }
  },
  "require": {
    "composer/installers": "~1.0",
    "pristas-peter/wp-graphql-gutenberg": "^0.3.8",
    "wpackagist-plugin/wp-graphql": "^1.3"
  }
}
